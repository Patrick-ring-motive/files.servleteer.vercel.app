{
  "headers": [
    {
      "source": "/(.*).jsonp",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/javascript"
        },
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }
      ]
    },{
      "source": "/(.*).jsn",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "application/json"
        }
      ]
    },{
      "source": "/(.*).js",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/javascript; charset=utf-8"
        },
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }
      ]
    },{
      "source": "/(.*).css",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/css"
        },
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }
      ]
    },{
      "source": "/(.*).html",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/html"
        },
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }
      ]
    }
  ]
}
