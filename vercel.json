{
  "headers": [
    {
      "source": "/(.*).jsonp",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/javascript"
        }
      ]
    },{
      "source": "/(.*).jsn",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "application/json"
        }
      ]
    },{
      "source": "/(.*).js",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/javascript"
        }
      ]
    },{
      "source": "/(.*).css",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/css"
        }
      ]
    },{
      "source": "/(.*).html",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/html"
        }
      ]
    },
    {
      "key" : "Access-Control-Allow-Origin",
      "value" : "*"
    }
  ]
}
